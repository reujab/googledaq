{
  "name": "googledaq",
  "version": "1.0.0",
  "description": "An idle game where you trade stocks in Google Trends",
  "main": "main.js",
  "scripts": {
    "start": "ELECTRON_DISABLE_SECURITY_WARNINGS=true electron .",
    "watch": "parcel watch -d dist --public-url . -t electron --no-hmr http/index.html",
    "build": "parcel build -d dist --public-url . -t electron --bundle-node-modules http/index.html",
    "prepack": "rm -rf dist && parcel build -d dist --public-url . -t electron --bundle-node-modules --no-source-maps http/index.html",
    "pack": "npm run prepack && electron-builder",
    "pack:linux": "npm run prepack && electron-builder --linux",
    "pack:win": "npm run prepack && electron-builder --win"
  },
  "author": "reujab <reujab@gmail.com>",
  "license": "Unlicense",
  "dependencies": {
    "@blueprintjs/core": "^3.15.0",
    "@blueprintjs/icons": "^3.7.0",
    "@material-ui/core": "^3.9.3",
    "chart.js": "^2.8.0",
    "google-trends-api": "^4.9.0",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "peity-react": "0.0.7",
    "react": "^16.8.6",
    "react-chartjs-2": "^2.7.6",
    "react-dom": "^16.8.6",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/chart.js": "^2.7.50",
    "@types/lodash": "^4.14.123",
    "@types/moment": "^2.13.0",
    "@types/node": "^11.13.0",
    "@types/react": "^16.8.12",
    "@types/react-chartjs-2": "^2.5.7",
    "@types/react-dom": "^16.8.3",
    "electron": "^4.1.4",
    "electron-builder": "^20.39.0",
    "electron-react-devtools": "^0.5.3",
    "parcel-bundler": "^1.12.3",
    "sass": "^1.17.4",
    "typescript": "^3.4.2"
  },
  "build": {
    "appImage": {
      "systemIntegration": "doNotAsk"
    },
    "compression": "store",
    "directories": {
      "output": "packages"
    },
    "files": [
      "!node_modules",
      "dist",
      "main.js"
    ],
    "linux": {
      "category": "Game",
      "icon": "googledaq.png"
    },
    "win": {
      "icon": "googledaq.ico",
      "target": "portable"
    }
  }
}
